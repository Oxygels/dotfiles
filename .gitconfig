# This is Git's per-user configuration file.
[user]
	name = Oxygels
	email = 39919597+Oxygels@users.noreply.github.com

[credential]
	helper = cache --timeout=3600

[pull]
	rebase = true

[alias]
	l = log --graph --oneline
    a = add
    d = diff
    ds = diff --staged
    s = status
    st = stash
    sp = stash pop
    c = commit
    ck = checkout
    cm = commit -m
    p = pull
    pu = push
